<%= render 'navigation' %>

<div class="container">
	<div class="row">
		<h3>Add Data</h3>
			<%= form_with model: @cats do |f|  %>
				<div class="form-group">
 					<%= f.label :Name %>
 					<%= f.text_field :name, class:'form-control', placeholder: "name", type:'text' %>
				</div>
				<div class="form-group">
	 				<%= f.label :Breed %> 	
	 				<%= f.text_field :breed, class:'form-control', placeholder: "breed", type:'text' %>
				</div>
				<div class="form-group">
			 		<%= f.label :Age %>	
			 		<%= f.text_field :age, class:'form-control', placeholder: "age", type:'number' %>
				</div>

			<%= f.submit 'Submit', class:'btn btn-primary' %>

		<% end %>
	</div>
</div>
